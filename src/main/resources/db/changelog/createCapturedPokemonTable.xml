<?xml version="1.0" encoding="UTF-8"?>

<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
           http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.9.xsd">

    <changeSet id="createCapturedPokemonTable" author="Kavin Chaisawangwong">
        <createTable tableName="captured_pokemon_table">
            <column name="trainerId" type="bigint" autoIncrement="true">
                <constraints primaryKey="true" primaryKeyName="trainerPk" />
            </column>
            <column name="capturedPokemon" type="varchar(250)"/>
        </createTable>

    </changeSet>


</databaseChangeLog>
